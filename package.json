{
  "name": "wongames-web",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint src --max-warnings=0",
    "typechecking": "tsc --project tsconfig.json --noEmit",
    "test": "jest --maxWorkers=50%",
    "test:watch": "yarn test --watch --maxWorkers=25%",
    "test:ci": "jest --runInBand",
    "cy:open": "cypress open",
    "schema:download": "apollo client:download-schema --endpoint=http://localhost:1337/graphql schema-temp.gql",
    "schema:convert": "node convertToNonNull.js",
    "codegen": "apollo codegen:generate --target typescript --excludes=node_modules/ --includes=src/graphql/**/*.ts --localSchemaFile schema.gql --tagName=gql --outputFlat src/graphql/generated",
    "types:generate": "yarn schema:download && yarn schema:convert && yarn codegen",
    "generate": "yarn plop --plopfile ./generators/plopfile.js",
    "storybook": "start-storybook -s ./public -p 6006",
    "build-storybook": "build-storybook -s ./public",
    "prepare": "husky install"
  },
  "lint-staged": {
    "src/**/*": [
      "yarn lint --fix",
      "yarn test --findRelatedTests --bail"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.7",
    "@stripe/react-stripe-js": "^1.4.1",
    "@stripe/stripe-js": "^1.15.0",
    "@styled-icons/boxicons-regular": "^10.23.0",
    "@styled-icons/boxicons-solid": "^10.18.0",
    "@styled-icons/fa-brands": "^10.22.0",
    "@styled-icons/material-outlined": "^10.18.0",
    "@styled-icons/remix-fill": "^10.18.0",
    "dotenv": "^9.0.2",
    "graphql": "^15.4.0",
    "joi": "^17.4.0",
    "lodash.xor": "^4.5.0",
    "next": "^10.2.2",
    "next-auth": "^3.23.2",
    "next-pwa": "^5.2.21",
    "next-seo": "^4.24.0",
    "nextjs-progressbar": "^0.0.9",
    "polished": "^4.1.2",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-slick": "^0.28.1",
    "styled-components": "5.3.0",
    "styled-media-query": "^2.1.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@babel/preset-typescript": "^7.12.1",
    "@storybook/addon-essentials": "6.2.9",
    "@storybook/react": "6.2.9",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^5.1.0",
    "@testing-library/user-event": "^13.1.9",
    "@types/jest": "^26.0.15",
    "@types/lodash.xor": "^4.5.6",
    "@types/node": "^15.3.1",
    "@types/react": "^17.0.2",
    "@types/react-slick": "^0.23.4",
    "@types/styled-components": "^5.1.4",
    "@typescript-eslint/eslint-plugin": "^4.5.0",
    "@typescript-eslint/parser": "^4.5.0",
    "apollo": "^2.32.1",
    "babel-loader": "^8.1.0",
    "babel-plugin-styled-components": "^1.11.1",
    "cypress": "^7.3.0",
    "eslint": "^7.11.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^6.0.0",
    "jest": "^26.6.3",
    "jest-styled-components": "^7.0.3",
    "lint-staged": "^10.4.2",
    "msw": "^0.28.2",
    "node-fetch": "^2.6.1",
    "plop": "^2.7.4",
    "prettier": "^2.1.2",
    "typescript": "^4.0.3"
  }
}
